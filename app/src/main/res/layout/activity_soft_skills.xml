<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:scrollbars="vertical"
		android:orientation="vertical"
		android:layout_margin="@dimen/activity_vertical_margin"
		tools:context=".ui.activity.SoftSkills">

	<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/black"
			android:textSize="@dimen/dimen_25sp"
			android:text="@string/teamwork"/>
	<TextView
			android:id="@+id/soft_teamwork"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/darker_gray"
			android:textSize="@dimen/dimen_20sp"
			android:layout_marginBottom="@dimen/dimen_20sp"/>
	<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/black"
			android:textSize="@dimen/dimen_25sp"
			android:text="@string/continuous_learning"/>
	<TextView
			android:id="@+id/soft_continuous"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/darker_gray"
			android:textSize="@dimen/dimen_20sp"
			android:layout_marginBottom="@dimen/dimen_20sp"/>
	<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/black"
			android:textSize="@dimen/dimen_25sp"
			android:text="@string/open_minded"/>
	<TextView
			android:id="@+id/soft_open_minded"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="@android:color/darker_gray"
			android:textSize="@dimen/dimen_20sp"/>

</LinearLayout>